{
    "name": "javascript-starter",
    "version": "2.0.1",
    "description": "A starter kit for building web projects with modern JavaScript",
    "author": {
        "name": "Amit Agarwal",
        "email": "amit@labnol.org",
        "url": "https://digitalinspiration.com/"
    },
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf ./dist",
        "format": "prettier --write src/**/*.{js,json,md}",
        "build": "parcel build index.html --public-url ./ --no-source-maps --no-scope-hoist",
        "start": "parcel"
    },
    "keywords": [
        "javascript",
        "webpack",
        "eslint",
        "prettier",
        "parcel",
        "es6",
        "babel",
        "vscode"
    ],
    "source": "index.html",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "targets": {
        "global": {
            "context": "browser",
            "outputFormat": "global"
        }
    },
    "engines": {
        "node": "16.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@parcel/transformer-sass": "^2.0.1",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "node-sass": "^6.0.1",
        "parcel": "^2.0.1",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2"
    },
    "dependencies": {
        "bootstrap": "5.0.2",
        "d3": "5.7.0",
        "graphml-js": "^0.4.0",
        "html-colors": "^0.0.6",
        "jquery": "3.2.1",
        "mathjax": "2.7.0",
        "pleasejs": "^0.4.2",
        "underscore": "^1.13.1",
        "xml-js": "1.6.11"
    }
}